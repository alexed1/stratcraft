<aura:component>
    <aura:registerEvent name="expressionsEvent" type="c:expressionsEvent" />
    <lightning:overlayLibrary aura:id="expressionsDialog" />
    <lightning:button name="cancel" label="Cancel" onclick="{!c.handleCancel}" />
    <lightning:button name="ok" label="Save" variant="brand" onclick="{!c.handleOK}" />

    <aura:handler event="c:expressionUpdatedEvent" action="{!c.handleExpressionUpdatedEvent }" />
    <aura:attribute name="expression" type="String" />
</aura:component>